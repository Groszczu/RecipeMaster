      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "setupFiles": [
      "<rootDir>/jestSetup.js"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
